{
  "name": "documentation",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "build": "rspress build",
    "check": "biome check --write",
    "dev": "rspress dev",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rspress preview",
    "commit": "cz",
    "bump": "cz bump"
  },
  "dependencies": {
    "rspress": "^1.40.2",
    "rspress-plugin-back-to-top": "^0.3.0",
    "rspress-plugin-mermaid": "^0.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.19.0",
    "@rspress/plugin-shiki": "^1.45.1",
    "@types/node": "^18.11.17",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.22.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "tag_format": "${version}",
      "version_scheme": "semver",
      "version_provider": "npm",
      "update_changelog_on_bump": true
    }
  }
}
